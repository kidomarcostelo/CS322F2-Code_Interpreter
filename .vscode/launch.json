{
  "version": "0.2.0",
  "configurations": [
    
    {
      "name": ".NET Core Launch (console)",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "build",
      "program": "${workspaceFolder}/CODE-Interpreter/bin/Debug/net7.0-windows/CODE-Interpreter.dll",
      "args": [],
      "cwd": "${workspaceFolder}/CODE-Interpreter",
      "console": "internalConsole",
      "stopAtEntry": false
    },
    {
      "name": ".NET Core Attach",
      "type": "coreclr",
      "request": "attach"
    },
    {
      "name": "Debug ANTLR4 grammar",
      "type": "antlr-debug",
      "request": "launch",
      "input": "CODE-Interpreter/Content/input.txt",
      "grammar": "CODE-Interpreter/Content/Code.g4",
      "startRule": "program",
      "printParseTree": true,
      "visualParseTree": true
    }
  ]
}